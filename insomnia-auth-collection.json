{
  "_type": "export",
  "version": 4,
  "workspace": {
    "_id": "wrk_pizzeria_auth",
    "name": "LA PIZZERÍA - Autenticación API",
    "description": "Colección de endpoints para el Módulo 1: Autenticación",
    "scope": "collection",
    "resources": [
      {
        "_id": "env_base",
        "parentId": "wrk_pizzeria_auth",
        "_type": "environment",
        "name": "Base Environment",
        "data": {
          "base_url": "http://localhost:8000",
          "token": ""
        }
      },
      {
        "_id": "req_register",
        "parentId": "fld_auth",
        "_type": "request",
        "name": "Register",
        "method": "POST",
        "url": "{{ base_url }}/api/auth/register",
        "headers": [
          {
            "name": "Content-Type",
            "value": "application/json"
          },
          {
            "name": "Accept",
            "value": "application/json"
          }
        ],
        "body": {
          "mimeType": "application/json",
          "text": "{\n  \"nombre\": \"Usuario Nuevo\",\n  \"email\": \"nuevo@lapizzeria.ec\",\n  \"telefono\": \"+593998765432\",\n  \"password\": \"NewPass@123\",\n  \"password_confirmation\": \"NewPass@123\"\n}"
        }
      },
      {
        "_id": "req_login",
        "parentId": "fld_auth",
        "_type": "request",
        "name": "Login",
        "method": "POST",
        "url": "{{ base_url }}/api/auth/login",
        "headers": [
          {
            "name": "Content-Type",
            "value": "application/json"
          },
          {
            "name": "Accept",
            "value": "application/json"
          }
        ],
        "body": {
          "mimeType": "application/json",
          "text": "{\n  \"email\": \"admin@lapizzeria.ec\",\n  \"password\": \"Admin@123456\"\n}"
        }
      },
      {
        "_id": "req_get_me",
        "parentId": "fld_auth",
        "_type": "request",
        "name": "Get Me",
        "method": "GET",
        "url": "{{ base_url }}/api/auth/me",
        "headers": [
          {
            "name": "Authorization",
            "value": "Bearer {{ token }}"
          },
          {
            "name": "Accept",
            "value": "application/json"
          }
        ]
      },
      {
        "_id": "req_verify_token",
        "parentId": "fld_auth",
        "_type": "request",
        "name": "Verify Token",
        "method": "GET",
        "url": "{{ base_url }}/api/auth/verify-token",
        "headers": [
          {
            "name": "Authorization",
            "value": "Bearer {{ token }}"
          },
          {
            "name": "Accept",
            "value": "application/json"
          }
        ]
      },
      {
        "_id": "req_change_password",
        "parentId": "fld_auth",
        "_type": "request",
        "name": "Change Password",
        "method": "POST",
        "url": "{{ base_url }}/api/auth/change-password",
        "headers": [
          {
            "name": "Authorization",
            "value": "Bearer {{ token }}"
          },
          {
            "name": "Content-Type",
            "value": "application/json"
          },
          {
            "name": "Accept",
            "value": "application/json"
          }
        ],
        "body": {
          "mimeType": "application/json",
          "text": "{\n  \"password_actual\": \"Admin@123456\",\n  \"password_nueva\": \"NewAdmin@789\",\n  \"password_nueva_confirmation\": \"NewAdmin@789\"\n}"
        }
      },
      {
        "_id": "req_forgot_password",
        "parentId": "fld_auth",
        "_type": "request",
        "name": "Forgot Password",
        "method": "POST",
        "url": "{{ base_url }}/api/auth/forgot-password",
        "headers": [
          {
            "name": "Content-Type",
            "value": "application/json"
          },
          {
            "name": "Accept",
            "value": "application/json"
          }
        ],
        "body": {
          "mimeType": "application/json",
          "text": "{\n  \"email\": \"admin@lapizzeria.ec\"\n}"
        }
      },
      {
        "_id": "req_reset_password",
        "parentId": "fld_auth",
        "_type": "request",
        "name": "Reset Password",
        "method": "POST",
        "url": "{{ base_url }}/api/auth/reset-password",
        "headers": [
          {
            "name": "Content-Type",
            "value": "application/json"
          },
          {
            "name": "Accept",
            "value": "application/json"
          }
        ],
        "body": {
          "mimeType": "application/json",
          "text": "{\n  \"email\": \"admin@lapizzeria.ec\",\n  \"token\": \"TOKEN_HERE\",\n  \"password\": \"ResetPass@456\",\n  \"password_confirmation\": \"ResetPass@456\"\n}"
        }
      },
      {
        "_id": "req_logout",
        "parentId": "fld_auth",
        "_type": "request",
        "name": "Logout",
        "method": "POST",
        "url": "{{ base_url }}/api/auth/logout",
        "headers": [
          {
            "name": "Authorization",
            "value": "Bearer {{ token }}"
          },
          {
            "name": "Content-Type",
            "value": "application/json"
          },
          {
            "name": "Accept",
            "value": "application/json"
          }
        ]
      },
      {
        "_id": "fld_auth",
        "parentId": "wrk_pizzeria_auth",
        "_type": "request_group",
        "name": "Auth Endpoints",
        "environment": {}
      }
    ]
  }
}
